

public class Solution {
    public List<List<Integer>> permute(int[] nums) {
        List res = new ArrayList();
        if(nums == null || nums.length == 0) return res;
        
        ArrayList<Integer> temp = new ArrayList<Integer>();
        help(res,temp,nums);
        return res;
    }
    private void help(List list, ArrayList<Integer> temp, int nums[]){
        if(temp.size() == nums.length){
            list.add(new ArrayList(temp));//注意java对于对象是传refer，对基础类型是传值，所以要新作一个list，要不然一个list改变，所有list都改变，
            return;
        }
        
        for(int i = 0; i < nums.length; i++){
            if(temp.contains(nums[i])) continue;
            
            temp.add(nums[i]);
            help(list,temp,nums);
            temp.remove(temp.size() - 1);//这里要记得移除加的位置，因为当前循环，这个temp的size是不变的
        }
    }
