public class Solution {
    public int rob(int[] nums) {
        int l = nums.length;
        
        int [] res = new int [l]; 
        if(l > 0){
            if(l == 1) return nums[0];
           
            res[0] = nums[0];
            res[1] = Math.max(nums[0], nums[1]);  
            for(int i = 2; i < l; i++){
                res[i] = Math.max(res[i - 1], res[i - 2] + nums[i]);
            } 
            return res[l - 1];
        }
       return 0; 
    }
}
